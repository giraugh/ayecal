{
  "name": "ayecal",
  "version": "0.1.7",
  "description": "Create and manipulate iCal files",
  "keywords": ["ical", "ics", "calendar", "webcal"],
  "repository": "https://github.com/stevent-team/ayecal",
  "author": "Stevent team (https://stevent.club)",
  "license": "MIT",
  "packageManager": "yarn@4.0.2",
  "type": "module",
